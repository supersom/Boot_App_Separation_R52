; app.scat â€“ application image at 0x00020000

LR_APP 0x00080000 0x00010000   ; 64 KB app region
{
    ER_APP   0x00080000 0x00010000
    {
        * (.app_vector_table, +First) ; Place vector table first
        .ANY (+RO)
    }

    SHARED_RW   0x00400100 UNINIT 0x0F00
    {
        * (BOOT_ARGS)
        * (FLAGS)
        * (VARS)
    }

    RW_APP   0x30040000 0x00008000
    {
        .ANY (+RW,+ZI)
    }

    ARM_LIB_HEAP 0x20040000 EMPTY 0x00004000 ; 16KB Heap
    {
    }

    ARM_LIB_STACK 0x20048000 EMPTY -0x00004000 ; 16KB Stack
    {
    }

    ; TCM regions (EMPTY so linker defines symbols)
    ATCM 0x0D000000 EMPTY 0x00020000
    {
    }
    BTCM 0x0E000000 EMPTY 0x00020000
    {
    }
    CTCM 0x0F000000 EMPTY 0x00020000
    {
    }
}
